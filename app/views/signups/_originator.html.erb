<%= form_for @signup, :html => {:class => "form-inline"} do |f| %>
      <%= f.text_field :email, :placeholder => "Leave your email to join", :class => "input-block-level"%>
      <%= f.hidden_field :originator, :value => true %> 
      <%= f.submit "as Originator", class: "btn btn-success" %>
<% end %>